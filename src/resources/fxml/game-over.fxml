<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EndGameCtrl">
   <children>
      <Label fx:id="gameOver" layoutX="199.0" layoutY="56.0" text="Game Over" textFill="#fffd00">
         <font>
            <Font name="Eras Bold ITC" size="36.0" />
         </font>
      </Label>
      <Label fx:id="nickName" alignment="CENTER" layoutX="181.0" layoutY="128.0" prefHeight="23.0" prefWidth="239.0" style="-fx-background-color: black;">
         <font>
            <Font name="Consolas" size="20.0" />
         </font>
      </Label>
      <Label fx:id="result" alignment="CENTER" layoutX="182.0" layoutY="161.0" prefHeight="23.0" prefWidth="239.0" style="-fx-background-color: black;">
         <font>
            <Font name="Consolas" size="20.0" />
         </font>
      </Label>
      <Label fx:id="score" alignment="CENTER" layoutX="169.0" layoutY="224.0" prefHeight="23.0" prefWidth="266.0" style="-fx-background-color: black;">
         <font>
            <Font name="Consolas" size="20.0" />
         </font>
      </Label>
      <Button fx:id="mainBtn" layoutX="225.0" layoutY="336.0" mnemonicParsing="false" onAction="#exit" prefHeight="38.0" prefWidth="152.0" style="-fx-background-color: fffd00;" text="Main Menu">
         <font>
            <Font name="Eras Bold ITC" size="20.0" />
         </font>
      </Button>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="26.0" layoutY="224.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/happy-pacman.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
